    "ts-loader": "^7.0.2",
    "typescript": "^3.9.0",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^3.6.3",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.2.3",
    "@microsoft/fast-tooling": "^0.25.0",
    "@microsoft/fast-tooling-react": "^2.9.3",
    "@microsoft/fast-foundation": "^2.5.0",
    "monaco-editor": "^0.21.2",
