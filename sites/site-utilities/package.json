{
  "name": "@microsoft/site-utilities",
  "version": "0.9.0",
  "description": "Utilities for the Creator and Component Explorer sites",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "clean:dist": "node ../../build/clean.js dist",
    "convert:mdn-data": "node -r esm ./build/generate-mdn-data-files.js",
    "test:mdn-data": "node -r esm ./build/generate-mdn-data-files.js --test",
    "generate:schemas": "node -r esm ./build/generate-component-schemas.js",
    "generate:badges": "node ./build/badges.js",
    "generate:icons": "node ./build/icons.js",
    "generate:all": "yarn generate:badges && yarn generate:icons && generate:schemas",
    "prepare": "yarn clean:dist && yarn generate:badges && yarn generate:schemas && yarn build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "test": "yarn eslint && yarn build",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast.git"
  },
  "author": "Microsoft",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@microsoft/eslint-config-fast-dna": "^1.2.0",
    "@types/node": "^9.6.7",
    "@types/react": "^16.8.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "badge-maker": "^3.2.0",
    "chalk": "^4.1.0",
    "csstype": "^2.5.8",
    "eslint-config-prettier": "6.10.1",
    "favicons": "^6.1.0",
    "fs-extra": "^9.0.1",
    "jest": "^25.4.0",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash-es": "4.17.15",
    "prettier": "^1.15.3",
    "raf-throttle": "^2.0.3",
    "react": "^16.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.9.0",
    "vscode-web-custom-data": "^0.3.1"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.2.3",
    "@microsoft/fast-foundation": "^2.5.0",
    "@microsoft/fast-tooling": "^0.25.0",
    "@microsoft/fast-tooling-react": "^2.9.3",
    "@skatejs/val": "^0.5.0",
    "monaco-editor": "^0.21.2",
    "vscode-html-languageservice": "^3.1.3"
  }
}
