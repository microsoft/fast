{
  "$schema": "http://json-schema.org/schema#",
  "$id": "fast-divider",
  "id": "fast-divider",
  "title": "Divider",
  "description": "The FAST divider element",
  "type": "object",
  "version": 1.1,
  "mapsToTagName": "fast-divider",
  "properties": {
    "role": {
      "enum": [
        "separator",
        "presentation"
      ],
      "default": "separator",
      "title": "Role",
      "description": "The ARIA role for the divider",
      "mapsToAttribute": "role",
      "type": "string"
    },
    "orientation": {
      "enum": [
        "horizontal",
        "vertical"
      ],
      "default": "horizontal",
      "title": "Orientation",
      "description": "The orientation of the divider",
      "mapsToAttribute": "orientation",
      "type": "string"
    }
  }
}