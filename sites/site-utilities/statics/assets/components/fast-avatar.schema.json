{
  "$schema": "http://json-schema.org/schema#",
  "$id": "fast-avatar",
  "id": "fast-avatar",
  "title": "Avatar",
  "description": "The FAST avatar element",
  "type": "object",
  "version": 1.1,
  "mapsToTagName": "fast-avatar",
  "properties": {
    "fill": {
      "description": "The fill attribute",
      "mapsToAttribute": "fill",
      "type": "string"
    },
    "color": {
      "description": "The color attribute",
      "mapsToAttribute": "color",
      "type": "string"
    },
    "alt": {
      "description": "The alt attribute",
      "mapsToAttribute": "alt",
      "type": "string"
    },
    "link": {
      "description": "The link attribute",
      "mapsToAttribute": "link",
      "type": "string"
    },
    "shape": {
      "default": "circle",
      "description": "The shape attribute",
      "mapsToAttribute": "shape",
      "type": "string"
    },
    "SlotBadge": {
      "description": "The badge slot",
      "mapsToSlot": "badge",
      "@microsoft/fast-tooling/dictionary-link": true,
      "pluginId": "@microsoft/fast-tooling/linked-data",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "title": "The ID of the data corresponding to a dictionary key",
            "type": "string"
          },
          "dataLocation": {
            "title": "The location of the data using lodash path syntax",
            "type": "string"
          }
        }
      }
    }
  }
}