{
    "name": "@microsoft/fast-benchmarks",
    "version": "1.0.0",
    "description": "utility library benchmarking for FAST libraries",
    "type": "module",
    "private": true,
    "author": {
        "name": "Microsoft",
        "url": "https://discord.gg/FcSNfg4"
    },
    "license": "MIT",
    "scripts": {
        "benchmark": "node scripts/index.js",
        "build": "tsc && tsc -p tsconfig.json",
        "help": "node ./scripts/index.js --help",
        "debug:binding": "tach --config fast-element/binding/tachometer.json --manual",
        "benchmark:fast-element:render": "yarn run benchmark --library=fast-element --benchmark=render --versions local latest --localBenchFile=index2"
    },
    "dependencies": {
        "@microsoft/fast-element": "^1.10.0",
        "tachometer": "^0.5.10"
    },
    "devDependencies": {
        "chalk": "^5.0.1",
        "chromedriver": "^101.0.0",
        "commander": "^9.2.0",
        "cross-spawn": "^7.0.3",
        "typescript": "^4.6.2"
    }
}
