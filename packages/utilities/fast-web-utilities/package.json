{
  "name": "@ni/fast-web-utilities",
  "description": "FAST web utilities",
  "version": "0.0.5",
  "sideEffects": false,
  "author": {
    "name": "National Instruments"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast/issues/new/choose"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean:dist && tsc -p ./tsconfig.build.json",
    "pack": "npm pack",
    "lint": "npm run eslint",
    "format": "npm run eslint:fix",
    "test": "npm run build && npm run test-chrome:verbose",
    "clean:dist": "rimraf dist",
    "prettier": "prettier --config ../../../.prettierrc --write \"**/*.ts\"",
    "prettier:diff": "prettier --config ../../../.prettierrc \"**/*.ts\" --list-different",
    "test-node": "mocha --reporter min --exit dist/esm/__test__/setup-node.js './dist/esm/**/*.spec.js'",
    "test-node:verbose": "mocha --reporter spec --exit dist/esm/__test__/setup-node.js './dist/esm/**/*.spec.js'",
    "test-chrome": "karma start karma.conf.ts --browsers=ChromeHeadlessOpt --single-run",
    "test-chrome:verbose": "karma start karma.conf.ts --browsers=ChromeHeadlessOpt --single-run --reporter=mocha",
    "test-chrome:watch": "karma start karma.conf.ts --browsers=ChromeHeadlessOpt --watch-extensions js",
    "test-chrome:debugger": "karma start karma.conf.ts --browsers=ChromeDebugging",
    "test-chrome:verbose:watch": "karma start karma.conf.ts --browsers=ChromeHeadlessOpt --watch-extensions js --reporter=mocha",
    "test-chrome:verbose:debugger": "karma start karma.conf.ts --browsers=ChromeDebugging --reporter=mocha",
    "test-firefox": "karma start karma.conf.ts --browsers=FirefoxHeadless --single-run",
    "test-firefox:verbose": "karma start karma.conf.ts --browsers=FirefoxHeadless --single-run --reporter=mocha",
    "test-firefox:watch": "karma start karma.conf.ts --browsers=FirefoxHeadless   --watch-extensions js",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "watch": "npm run build -- -w --preserveWatchOutput"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/chai-spies": "^1.0.3",
    "@types/karma": "^6.3.9",
    "@types/mocha": "^10.0.10",
    "@ni/eslint-config-fast-dna": "^1.0.0",
    "chai": "^4",
    "chai-spies": "^1.0.0",
    "eslint-config-prettier": "^10.0.1",
    "jsdom": "^26.0.0",
    "jsdom-global": "3.0.2",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.4.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.1",
    "mocha": "^11.0.1",
    "playwright": "1.44.0",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "exenv-es6": "^1.1.1"
  }
}
