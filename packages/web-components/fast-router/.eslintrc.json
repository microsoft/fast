{
    "extends": ["../../../.eslintrc.js"],
    "rules": {
        "@typescript-eslint/no-non-null-assertion": "off",
        "@typescript-eslint/ban-types": [
            "error",
            {
                "types": {
                    "{}": false,
                    "Function": false,
                    "Object": false
                },
                "extendDefaults": true
            }
        ]
    }
}
