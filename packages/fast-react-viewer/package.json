{
  "name": "@microsoft/fast-react-viewer",
  "version": "1.0.0",
  "description": "An initial implementation of a React iframe that is self-contained.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ts-node helpers/unitTests.ts",
    "test:update": "ts-node helpers/unitTests.ts --update",
    "dev-server": "webpack-dev-server --progress",
    "build": "tsc -p ./src/tsconfig.json",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "reactjs"
  ],
  "author": {
    "name": "Fluent Web",
    "email": "fwq@microsoft.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@microsoft/fast-tslint-rules": "^1.0.2",
    "@types/jest": "^22.1.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.3",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.3",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*(test|spec))\\.(ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./src/tsconfig.json"
      }
    }
  },
  "dependencies": {
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
