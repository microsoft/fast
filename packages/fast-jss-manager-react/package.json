{
  "name": "@microsoft/fast-react-jss-manager",
  "description": "React HOC for managing component JSS stylesheets",
  "author": {
    "name": "Fluent Web",
    "email": "fwq@microsoft.com"
  },
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch && npm-watch",
    "lint": "tslint -c tslint.json ./src/**/*.ts{,x}",
    "lint:fix": "tslint -c tslint.json ./src/**/*.ts{,x} --fix",
    "lint:watch": "node build/tslint-watch.js",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "readme": "README.md",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "devDependencies": {
    "@microsoft/fast-tslint-rules": "^1.0.4",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "jest": "^22.3.0",
    "react": "^16.0.0",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0",
    "uuid": "^3.2.1"
  }
}
