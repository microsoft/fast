{
  "name": "@microsoft/fast-storybook-presets",
  "version": "1.2.6",
  "description": "Storybook presets for FAST integration",
  "main": "dist/index.js",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "scripts": {
    "prepare": "tsc -p ./tsconfig.json",
    "prettier": "prettier --config ../../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "test": "yarn eslint",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "watch": "tsc -p ./tsconfig.json -w --preserveWatchOutput"
  },
  "dependencies": {
    "@microsoft/fast-components-styles-msft": "^4.28.13",
    "@microsoft/fast-storybook-design-system-addon": "^1.2.38",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "glob": "^7.1.4",
    "lodash": "^4.17.11",
    "storybook-addon-performance": "^0.7.0"
  },
  "peerDependencies": {
    "@microsoft/fast-jss-manager-react": "^4.3.5",
    "@storybook/react": "^5.1.3",
    "react": "^16.6.0"
  },
  "devDependencies": {
    "@microsoft/eslint-config-fast-dna": "^1.1.3",
    "@storybook/react": "^5.1.3",
    "@storybook/theming": "^5.1.3",
    "@types/react": "^16.8.19",
    "@types/webpack-env": "^1.14.0",
    "eslint-config-prettier": "^6.10.1",
    "prettier": "2.0.2",
    "react": "^16.8.6",
    "typescript": "^3.5.3"
  }
}
